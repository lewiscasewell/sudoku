{
  "name": "sudoku",
  "private": true,
  "scripts": {
    "db-generate": "turbo db-generate",
    "db-push": "turbo db-push",
    "build": "turbo build",
    "build:ios:production": "turbo build:ios:production",
    "submit:ios": "turbo submit:ios",
    "dev": "dotenv -- turbo dev --parallel",
    "clean:workspaces": "turbo clean",
    "clean": "rm -rf node_modules .turbo .vercel",
    "seed": "turbo seed",
    "lint": "turbo lint && manypkg check",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "dependencies": {
    "@manypkg/cli": "^0.20.0",
    "@typescript-eslint/parser": "^5.57.0",
    "dotenv-cli": "^7.1.0",
    "eslint": "^8.36.0",
    "prettier": "^2.8.7",
    "turbo": "^1.8.6",
    "typescript": "^5.0.2"
  }
}
